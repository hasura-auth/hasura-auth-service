{
  "include": ["src/**/*.ts"],
  "exclude": [
    "src/db/migrations/**",
    "src/index.ts",
    "src/config/logger.ts",
    "src/db/config.js",
    "src/model/**",
    "src/pages/**"
  ],
  "extension": [".js", ".ts"],
  "all": true,
  "reporter": ["html", "text"],
  "report-dir": "./test/.coverage",
  "temp-dir": "./test/.nyc_output",
  "sourceMap": false,
  "cache": true,
  "require": ["ts-node/register"],
  "check-coverage": true,
  "branches": 70,
  "lines": 70,
  "functions": 70,
  "statements": 70
}
